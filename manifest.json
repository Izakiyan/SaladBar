{
  "manifest_version": 2,

  "name": "SaladBar",
  "description": "This extension checks the 10bis checkboxes for you",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["https://www.10bis.co.il/*"],
      "js": ["jquery-2.2.1.min.js", "fuck.js"]
    }
  ],

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}
